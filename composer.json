{
    "name": "tomatophp/filament-fcm-driver",
    "type": "library",
    "description": "Firebase Cloud Messaging driver for Filament Alerts Sender",
    "keywords": [
        "php",
        "laravel",
        "template"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\FilamentFcmDriver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TomatoPHP\\FilamentFcmDriver\\Tests\\": "tests/src/",
            "TomatoPHP\\FilamentFcmDriver\\Tests\\Database\\Factories\\": "tests/database/factories"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\FilamentFcmDriver\\FilamentFcmDriverServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "scripts": {
        "testbench": "vendor/bin/testbench package:discover --ansi",
        "db": "vendor/bin/testbench package:create-sqlite-db && vendor/bin/testbench migrate",
        "analyse": "vendor/bin/phpstan analyse src tests",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "require": {
        "php": "^8.1|^8.2",
        "filament/filament": "^3.2",
        "filament/notifications": "^3.2",
        "tomatophp/console-helpers": "^1.1",
        "mobiledetect/mobiledetectlib": "^4.8",
        "laravel-notification-channels/fcm": "^5.1",
        "mallardduck/blade-boxicons": "^2.4"
    },
    "require-dev": {
       "laravel/pint": "^1.18",
       "livewire/livewire": "^2.10|^3.0",
       "nunomaduro/larastan": "^2.9",
       "orchestra/testbench": "^9.5",
       "pestphp/pest": "^2.36",
       "pestphp/pest-plugin-laravel": "^2.4",
       "pestphp/pest-plugin-livewire": "^2.1",
       "phpstan/extension-installer": "^1.4",
       "phpstan/phpstan-deprecation-rules": "^1.2",
       "phpstan/phpstan-phpunit": "^1.4"
    },
    "version": "v1.1.0"
}
